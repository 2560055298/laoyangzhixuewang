<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atguigu.ucenterservice.mapper.UcenterMemberMapper">
    <select id="selectCountByCreateTime" resultType="int" parameterType="string">
        select count(*) from guli.ucenter_member
        where date(gmt_create) = #{gmtCreate}
    </select>
</mapper>
